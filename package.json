{
  "name": "whatsprintisit",
  "displayName": "What Sprint Is It?",
  "description": "Azure DevOps sprint information right in the Visual Studio Code status bar.",
  "icon": "assets/WhatSprintIsIt.png",
  "version": "0.0.1",
  "publisher": "lszomoru",
  "license": "MIT",
  "engines": {
    "vscode": "^1.30.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lszomoru/vscode-whatsprintisit.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.openWhatSprintIsIt",
        "title": "What sprint is it?"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "open": "6.0.0",
    "rest": "2.0.0"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.37",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
